{
    "name": "brondby/roboticket-sdk",
    "description": "A SDK for working with the Roboticket API",
    "keywords": [
        "brondby",
        "roboticket-sdk"
    ],
    "homepage": "https://github.com/brondby/roboticket-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Troels Johnsen",
            "email": "tjo@brondby.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "brondby/saloon-helpers": "^1.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "nesbot/carbon": "^3.5",
        "saloonphp/laravel-plugin": "^3.0",
        "saloonphp/pagination-plugin": "^2.0",
        "saloonphp/saloon": "^3.0",
        "spatie/laravel-data": "^4.7"
    },
    "require-dev": {
        "pestphp/pest": "^3.1",
        "laravel/pint": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/brondby-if/saloon-helpers"
        }
    ],
    "autoload": {
        "psr-4": {
            "Brondby\\Roboticket\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brondby\\Roboticket\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
