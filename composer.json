{
    "name": "brondby/roboticket-sdk",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Brondby\\Roboticket\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brondby\\Roboticket\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Troels Johnsen",
            "email": "tjo@brondby.com"
        }
    ],
    "require": {
        "illuminate/contracts": "^10.0||^11.0",
        "saloonphp/laravel-plugin": "^3.0",
        "saloonphp/pagination-plugin": "^2.0",
        "saloonphp/saloon": "^3.0",
        "brondby/saloon-helpers": "dev-main",
        "nesbot/carbon": "^3.5"
    },
    "require-dev": {
        "pestphp/pest": "^2.34"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/brondby-if/saloon-helpers"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
